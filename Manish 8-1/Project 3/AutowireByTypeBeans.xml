<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="add1" class= "com.deloitte.fourthmvn.firstspringapp.beans.Address">
<property name="hno" value="3-2-1"/>
<property name="city" value="Vizag"/>
<property name="state" value="AP"/>
</bean>
<bean id="per1" class="com.deloitte.fourthmvn.firstspringapp.beans.Person" autowire="byType">
<property name="ssn" value="101"/>
<property name="name" value="Manish"/>
</bean>
</beans>